{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npx prisma generate && npm run build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "false"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  }
}
